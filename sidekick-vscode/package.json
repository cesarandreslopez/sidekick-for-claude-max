    "submenus": [
      {
        "id": "sidekick.menu",
        "label": "Sidekick"
      },
      {
        "id": "sidekick.explainSubmenu",
        "label": "Explain Code"
      },
      {
        "id": "sidekick.explainErrorSubmenu",
        "label": "Explain Error with AI"
      }
    ],
    "keybindings": [
      {
        "command": "sidekick.triggerCompletion",
        "key": "ctrl+shift+space",
        "mac": "cmd+shift+space",
        "when": "editorTextFocus"
      },
      {
        "command": "sidekick.transform",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "editorTextFocus && editorHasSelection"
      },
      {
        "command": "sidekick.generateDocs",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "sidekick.explainCode",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "editorTextFocus && editorHasSelection"
      },
      {
        "command": "sidekick.inlineChat",
        "key": "ctrl+i",
        "mac": "cmd+i",
        "when": "editorTextFocus && !editorReadonly && !suggestWidgetVisible && !inlineChatFocused"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "sidekick.reviewChanges",
          "group": "navigation",
          "when": "scmProvider == git"
        },
        {
          "command": "sidekick.generatePrDescription",
          "group": "navigation",
          "when": "scmProvider == git"
        },
        {
          "command": "sidekick.generateCommitMessage",
          "group": "navigation",
          "when": "scmProvider == git && config.sidekick.showCommitButton"
        }
      ],
      "editor/context": [
        {
          "submenu": "sidekick.menu",
          "group": "1_sidekick@1",
          "when": "editorTextFocus"
        }
      ],
      "sidekick.menu": [
        {
          "command": "sidekick.inlineChat",
          "group": "0_ask@1",
          "when": "editorTextFocus"
        },
        {
          "command": "sidekick.generateDocs",
          "group": "1_docs@1",
          "when": "editorTextFocus"
        },
        {
          "submenu": "sidekick.explainSubmenu",
          "group": "2_explain@1",
          "when": "editorHasSelection"
        },
        {
          "submenu": "sidekick.explainErrorSubmenu",
          "group": "2_explain@2",
          "when": "editorHasSelection"
        },
        {
          "command": "sidekick.fixError",
